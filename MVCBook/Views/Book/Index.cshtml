@model IEnumerable<MVCBook.Models.Book>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>@Html.ActionLink("Create new", "create", "Book")</p>
    <table>
        <tr>
            <th>@Html.DisplayNameFor(model=>model.Name)</th>    
            <th>@Html.DisplayNameFor(model=>model.Author)</th>    
            <th>@Html.DisplayNameFor(model=>model.PagesNumber)</th>    
            <th>@Html.DisplayNameFor(model=>model.Publisher)</th>    
            <th>@Html.DisplayNameFor(model=>model.PublicationDate)</th>    
            <th>@Html.DisplayNameFor(model=>model.Price)</th>           
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(ModelItem=>item.Name)</td>
                <td>@Html.DisplayFor(ModelItem=>item.Author)</td>
                <td>@Html.DisplayFor(ModelItem=>item.PagesNumber)</td>
                <td>@Html.DisplayFor(ModelItem=>item.Publisher)</td>
                <td>@Html.DisplayFor(ModelItem=>item.PublicationDate)</td>
                <td>@Html.DisplayFor(ModelItem=>item.Price)</td>                
            </tr>
        }
    </table>
</body>
</html>
